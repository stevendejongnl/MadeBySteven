  "type": "module",
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@rollup/plugin-beep": "^1.0.3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/node": "^24.0.8",
    "lit": "^3.3.0",
    "reload": "^3.3.0",
    "rollup": "^4.44.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
